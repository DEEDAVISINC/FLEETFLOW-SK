        {selectedTab === 'intelligence' && <IntelligenceTab stats={stats} />}
        {selectedTab === 'operations' && <OperationsTab />}
        {selectedTab === 'crossborder' && (
          <div style={{ display: 'grid', gap: '32px' }}>
            <div>
              <h2
