{
  "name": "fleetflow",
  "version": "1.0.0",
  "main": "index.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build --no-lint",
    "build:simple": "next build --no-lint",
    "start": "next start",
    "seed": "node scripts/seed-database.js",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit --skipLibCheck",
    "build:check": "npm run type-check && npm run build",
    "analyze": "cross-env ANALYZE=true npm run build",
    "clean": "rm -rf .next && rm -rf out",
    "sync-square": "node scripts/sync-square.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check:all": "npm run type-check && npm run lint && npm run format:check",
    "fix:all": "npm run format && npm run lint:fix",
    "pre-commit": "npm run check:all",
    "types:generate": "./scripts/generate-types-enhanced.sh development",
    "types:generate:all": "./scripts/generate-types.sh development && ./scripts/generate-types.sh staging && ./scripts/generate-types.sh production",
    "types:generate:prod": "./scripts/generate-types-enhanced.sh production",
    "types:generate:staging": "./scripts/generate-types-enhanced.sh staging",
    "types:watch": "./scripts/generate-types-enhanced.sh local watch",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:restart": "supabase stop && supabase start",
    "supabase:reset": "supabase db reset",
    "supabase:status": "supabase status",
    "db:migrate": "supabase db diff --schema public,storage --use-migra > supabase/migrations/20250720205604_schema_changes.sql",
    "db:migrate:up": "supabase migration up",
    "db:migrate:new": "supabase migration new",
    "db:push": "supabase db push",
    "db:pull": "supabase db pull",
    "functions:serve": "supabase functions serve --env-file .env.local",
    "functions:deploy": "supabase functions deploy",
    "functions:logs": "supabase functions logs",
    "dev:full": "concurrently \"npm run supabase:start\" \"npm run functions:serve\" \"npm run types:watch\" \"npm run dev\"",
    "dev:db": "concurrently \"npm run supabase:start\" \"npm run dev\"",
    "dev:types": "concurrently \"npm run types:watch\" \"npm run dev\"",
    "db:seed": "supabase db seed",
    "db:seed:dev": "supabase db reset && supabase db seed",
    "types:local": "supabase gen types typescript --local > lib/types/supabase-local.ts",
    "types:generate:local": "./scripts/generate-types-enhanced.sh local",
    "types:generate:dev": "./scripts/generate-types-enhanced.sh development",
    "dev:hot": "./scripts/dev-workflow.sh",
    "dev:local": "concurrently \"supabase start\" \"npm run types:watch\" \"npm run dev\"",
    "migrate": "./scripts/migration-manager.sh",
    "db:setup": "supabase start && ./scripts/migration-manager.sh"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
